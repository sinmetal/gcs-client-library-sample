<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>sinpkmnms</application>
  <version>2013-0625-1826</version>
  
  <static-files>
  	<include path="**.html" />
  	<include path="**.js" />
  	<include path="**.css" />
  	<include path="**.jpeg" />
  	<include path="**.jpg" />
  	<include path="**.gif" />
  	<include path="**.png" />
  </static-files>
  
  <precompilation-enabled>true</precompilation-enabled>
    
  <system-properties>
    <property name="slim3.hotReloading" value="true"/>
    <!--
    <property name="slim3.datastoreDeadline" value="8"/>
    <property name="slim3.uploadSizeMax" value="1000000"/>
    <property name="slim3.uploadFileSizeMax" value="100000"/>
    -->
    <property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties"/>
  </system-properties>
  <sessions-enabled>false</sessions-enabled>
  <threadsafe>true</threadsafe>
</appengine-web-app>
